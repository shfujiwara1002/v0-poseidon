# Poseidon.AI Prototype Environment Variables
# Copy this file to .env.local for local development

# API Configuration
VITE_API_URL=https://api.poseidon.ai
VITE_MOCK_API=true

# Feature Flags
VITE_FEATURE_CODE_SPLITTING=true
VITE_FEATURE_CONTEXT_API=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Build Information
VITE_VERSION=1.0.0
VITE_BUILD_ENV=development

# Authentication (for future backend integration)
# VITE_AUTH_PROVIDER=auth0
# VITE_AUTH_DOMAIN=poseidon.auth0.com
# VITE_AUTH_CLIENT_ID=your-client-id

# Monitoring & Analytics (for future integration)
# VITE_SENTRY_DSN=your-sentry-dsn
# VITE_GA_TRACKING_ID=your-ga-id
