{
  "name": "poseidon-prototype",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "verify:dashboard-artifacts": "node scripts/verify-dashboard-artifact-freshness.mjs",
    "deploy:verify-dashboard": "npm run build && npm run verify:dashboard-artifacts",
    "check:visual-system": "node scripts/check-page-visual-system.mjs",
    "check:visual-system:strict": "node scripts/check-page-visual-system.mjs --strict",
    "check:design-system:usage": "node scripts/check-design-system-usage.mjs",
    "check:design-system:tokens": "node scripts/check-design-token-hygiene.mjs",
    "check:design-system": "npm run check:design-system:strict",
    "check:design-system:strict": "node scripts/check-design-system-usage.mjs --strict && node scripts/check-design-token-hygiene.mjs --strict",
    "generate:registry": "node scripts/generate-registry-json.mjs",
    "check:registry-sync": "node scripts/generate-registry-json.mjs --check",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:a11y:smoke": "node scripts/a11y-smoke.mjs",
    "check:a11y-structure": "node scripts/check-a11y-structure.mjs",
    "check:cta-hierarchy": "node scripts/check-cta-hierarchy.mjs",
    "check:contrast-budget": "node scripts/check-contrast-budget.mjs",
    "test:lighthouse": "node scripts/lighthouse-ci.mjs",
    "verify:pwa": "node scripts/verify-pwa.mjs",
    "check:motion-policy": "node scripts/check-motion-policy.mjs",
    "check:bundle-budget": "node scripts/check-bundle-budget.mjs",
    "check:microcopy-tone": "node scripts/check-microcopy-tone.mjs",
    "ux:scan": "node scripts/ux-scan.mjs",
    "ux:scan:strict": "node scripts/ux-scan.mjs --strict",
    "ux:visual:capture": "node scripts/ux-visual-capture.mjs",
    "ux:visual:capture:strict": "node scripts/ux-visual-capture.mjs --strict",
    "ux:visual:diff": "node scripts/ux-visual-diff.mjs",
    "ux:visual:diff:strict": "node scripts/ux-visual-diff.mjs --strict",
    "ux:autofix:safe": "node scripts/ux-autofix.mjs --mode safe",
    "ux:verify": "node scripts/ux-verify.mjs",
    "ux:report": "node scripts/ux-report.mjs",
    "ux:cycle": "node scripts/ux-cycle.mjs",
    "check:contracts": "vitest run src/contracts/__tests__/screen-contracts.test.tsx",
    "test:coverage": "vitest run --coverage",
    "analyze": "vite build --mode analyze",
    "ds:check:tokens": "node scripts/check-ds-tokens.mjs",
    "ds:check:structure": "node scripts/check-ds-structure.mjs",
    "ds:check:quality": "node scripts/check-ds-quality.mjs",
    "ds:report": "node scripts/ds-compliance-report.mjs",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/container-queries": "^0.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.33.0",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.18",
    "terser": "^5.46.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.18"
  }
}
